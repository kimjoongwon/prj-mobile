	"scripts": {
		"start": "expo start",
		"start:clear": "expo start --clear",
		"ios": "expo run:ios",
		"android": "expo run:android",
		"web": "expo start --web",
		"web:clear": "rm -rf .vite && expo start --web",
		"ios:clear": "expo start --clear && expo run:ios",
		"android:clear": "expo start --clear && expo run:android",
		"lint": "expo lint",
		"storybook-generate": "sb-rn-get-stories",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"storybook:native": "EXPO_PUBLIC_STORYBOOK=true expo start",
		"storybook:native:ios": "EXPO_PUBLIC_STORYBOOK=true expo run:ios",
		"storybook:native:android": "EXPO_PUBLIC_STORYBOOK=true expo run:android",
		"prebuild": "expo prebuild",
		"prebuild:clean": "expo prebuild --clean",
		"prebuild:ios": "expo prebuild --platform ios",
		"prebuild:android": "expo prebuild --platform android",
		"prebuild:ios:clean": "expo prebuild --platform ios --clean",
		"prebuild:android:clean": "expo prebuild --platform android --clean"
	},
